{
  "name": "gulf-editor-codemirror",
  "version": "5.0.0",
  "description": "Convenient gulf wrapper for codemirror (uses shareaJS's textOT)",
  "main": "index.js",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "core-js": "2.x"
  },
  "peerDependencies": {
    "gulf": "5.x",
    "ot-text": "1.x"
  },
  "main": "./lib/index",
  "scripts": {
    "build": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": ["transform-runtime"]
  },
  "devDependencies": {
    "babel-cli": "6.x",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelklehr/gulf-editor-codemirror.git"
  },
  "keywords": [
    "operational transformation",
    "dom-ot",
    "codemirror"
  ],
  "author": "Marcel Klehr <mklehr@gmx.net>",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/marcelklehr/gulf-editor-codemirror/issues"
  },
  "homepage": "https://github.com/marcelklehr/gulf-editor-codemirror#readme"
}
